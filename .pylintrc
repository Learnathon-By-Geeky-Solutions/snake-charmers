[MASTER]
ignore=venv
jobs=1
ignored-modules=alembic.context, alembic.op
ignore-patterns=alembic/env.py

[MESSAGES CONTROL]
disable=missing-docstring,
        invalid-name,
        too-few-public-methods,
        missing-final-newline,
        import-error,
        no-name-in-module
        
[REPORTS]
output-format=colorized
reports=no

[FORMAT]
max-line-length=88